  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "3.0.0",
    "@tailwindcss/cli": "^4.0.8",
    "@tailwindcss/postcss": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.0.8"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "openseadragon": "^5.0.1"
  }
